<h1>Welcome to <span style="color: #191970">Blue-Magpie Acebook!</span></h1>
<h3>Log In</h3>
<p><%=form_for :user do|form| %>

  <div>
    <%= form.label :email %><br>
    <%= form.text_field :email %>
  </div>

  <div>
    <%= form.label :password %><br>
    <%= form.text_field :password %>
  </div>

  <div>
    <%= form.submit %>
  </div>
<% end %>
</p>
<a href= "/sign_up">Sign up!</a>

